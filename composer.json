{
    "name": "cronario/result",
    "description": "Result exceptions",
    "type": "library",
    "keywords": [
        "results",
        "exceptions"
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"

    },
    "autoload": {
        "psr-4": {
            "Result\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Result\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "bin": [
        "result"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.*-dev"
        }
    }
}
